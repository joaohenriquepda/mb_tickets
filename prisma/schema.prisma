generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["linux-musl-arm64-openssl-3.0.x", "native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  date        DateTime
  start_time  DateTime
  end_time    DateTime
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  name     String
}
